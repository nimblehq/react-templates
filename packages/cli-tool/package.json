{
  "name": "@nimblehq/react-templates",
  "version": "0.1.0",
  "description": "Nimble React Application Generator",
  "author": "Nimble <dev@nimblehq.co>",
  "bin": {
    "nimble-react ": "./bin/run"
  },
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/nimblehq/react-templates/packages/cli-tool",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "nimblehq/react-templates",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "cli-ux": "^6.0.5",
    "inquirer": "^8.2.4",
    "inquirer-test": "^2.0.1"
  },
  "devDependencies": {
    "@nimblehq/eslint-config-nimble": "^2.3.0",
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/inquirer": "^8.2.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.8.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "nimble-react",
    "dirname": "nimble-react",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {},
    "hooks": {},
    "additionalHelpFlags": [
      "-h"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && shx cp -r src/extensions dist",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint-fix": "eslint . --fix --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/nimblehq/react-templates/issues",
  "types": "dist/index.d.ts"
}
